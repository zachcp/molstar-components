{
  "name": "@zachcp/molstar-components",
  "version": "0.4.0",
  "license": "MIT",
  "exports": "./src/mod.ts",
  "imports": {
    "preact": "npm:preact@10.19.3",
    "preact/hooks": "npm:preact@10.19.3/hooks",
    "preact/jsx-runtime": "npm:preact@10.19.3/jsx-runtime",
    "@preact/signals": "npm:@preact/signals@1.2.2",
    "@molstar/mol-view-stories": "jsr:@molstar/mol-view-stories@1.0.1"
  },
  "tasks": {
    "bundle": "deno bundle src/mod.ts -o docs/molstar-components.dev.js --platform browser --external preact --external preact/hooks --external preact/jsx-runtime --external @preact/signals",
    "serve": "deno run --allow-net --allow-read jsr:@std/http/file-server -p 8000",
    "dev": "deno task bundle && deno task serve"
  },
  "exclude": ["docs/", ".github/"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "lib": ["dom", "dom.iterable", "es2022"]
  }
}
