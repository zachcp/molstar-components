{
  "name": "@zachcp/molstar-components",
  "version": "0.4.8",
  "license": "MIT",
  "exports": "./src/mod.ts",
  "imports": {
    "preact": "npm:preact@10.28.1",
    "preact/hooks": "npm:preact@10.28.1/hooks",
    "preact/jsx-runtime": "npm:preact@10.28.1/jsx-runtime",
    "@preact/signals": "npm:@preact/signals@2.5.1",
    "@molstar/mol-view-stories": "jsr:@molstar/mol-view-stories@1.0.3",
    "monaco-editor": "npm:monaco-editor@0.55.1/esm/vs/editor/editor.api.js",
    "monaco-editor/typescript": "npm:monaco-editor@0.55.1/esm/vs/language/typescript/monaco.contribution.js",
    "esbuild": "npm:esbuild@0.27.2",
    "@luca/esbuild-deno-loader": "jsr:@luca/esbuild-deno-loader@^0.11.1",
    "@std/path": "jsr:@std/path@^1.1.4"
  },
  "tasks": {
    "clean": "rm -f docs/bundle.js docs/bundle.js.map docs/bundle.css docs/molstar-components.js docs/molstar-components.js.map docs/molstar-components.css && rm -rf docs/assets",
    "build": "deno task clean && deno run --allow-all build.ts",
    "serve": "deno run --allow-net --allow-read jsr:@std/http/file-server --host 127.0.0.1 -p 8000",
    "dev": "deno task build && deno task serve"
  },
  "exclude": ["docs/", ".github/"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "preact",
    "lib": ["dom", "dom.iterable", "es2022"]
  }
}
